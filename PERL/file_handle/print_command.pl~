#!/use/bin/perl
print ("Enter the value of a:$a \n");
$a=<STDIN>;
print ("Enter the value of b:$b \n");
$b=<STDIN>;
print ("Enter the value of c:$c \n");
$c=<STDIN>;
print ("Enter the value of e:$e \n");
$e=<STDIN>;
print ("Enter the value of f:$f \n");
$f=<STDIN>;
$z=($a*$b/$c+$e)-$f;
print ("Equation result of $z\n");
